{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "object",
    "title": "The Root Schema",
    "required": [
      "id",
      "title"
    ],
    "properties": {
      "id": {
        "type": "integer",
        "title": "The Id Schema",
        "default": 0,
        "examples": [
          1
        ]
      },
      "title": {
        "type": "string",
        "title": "The Title Schema",
        "default": "",
        "examples": [
          "test"
        ],
        "pattern": "^(.*)$"
      }
    }
  }